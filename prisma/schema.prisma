generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Mentor {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  bio        String
  company    String
  cost       Int
  experience Int
  img        String
  name       String
  occupation String
  tags       String[]
  meeting    Meeting[]
}

model Meeting {
  id         String   @id @default(auto()) @map("_id") @db.ObjectId
  mentor     Mentor?  @relation(fields: [mentorId], references: [id])
  mentorId   String   @db.ObjectId
  date       String
  time       String
}
